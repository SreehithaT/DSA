import java.util.*;
import java.util.Arrays;
import java.util.Collections;
class Solution {
    public static int rakshaBandhan(int arr[], int n) {
        // Write your code here.
        Arrays.sort(arr);
        long sum=0;
        int c=0;
        for(int i=n-1;i>=0;i--){
            sum=sum+arr[i];
            if(sum<=0){
               break;
            }
            c++;
        }
        return c;
    }
}
